{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://roomloop-server.onrender.com/api",
    "REACT_APP_SOCKET_URL": "https://roomloop-server.onrender.com"
  }
}
